<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_detail_member"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="nuel.handoyo.myPharm.DetailMemberActivity">

    <TextView
        android:id="@+id/textViewA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:text="MemberCode" />

    <TextView
        android:id="@+id/textViewAT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewA"
        android:layout_alignLeft="@+id/textViewA"
        android:layout_marginTop="10dp"
        android:background="@drawable/back"
        android:padding="5dp"
        android:text="[membercode]" />

    <TextView
        android:id="@+id/textViewB"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewAT"
        android:layout_alignLeft="@+id/textViewAT"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:text="Username" />

    <TextView
        android:id="@+id/textViewBT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewB"
        android:layout_alignLeft="@+id/textViewB"
        android:background="@drawable/back"
        android:padding="5dp"
        android:text="[username]" />

    <TextView
        android:id="@+id/textViewC"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textViewBT"
        android:layout_alignLeft="@+id/textViewBT"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:text="Nama Lengkap" />

    <TextView
        android:id="@+id/textViewCT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewC"
        android:layout_alignLeft="@+id/textViewC"
        android:layout_marginTop="10dp"
        android:background="@drawable/back"
        android:padding="5dp"
        android:text="[fullname]" />

    <TextView
        android:id="@+id/textViewD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textViewCT"
        android:layout_alignLeft="@+id/textViewCT"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:text="Tanggal Lahir" />

    <TextView
        android:id="@+id/textViewDT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewD"
        android:layout_alignLeft="@+id/textViewD"
        android:layout_marginTop="10dp"
        android:background="@drawable/back"
        android:padding="5dp"
        android:text="[birthdate]" />

    <TextView
        android:id="@+id/textViewF"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textViewDT"
        android:layout_alignLeft="@+id/textViewDT"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:text="Jenis Kelamin" />

    <TextView
        android:id="@+id/textViewFT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewF"
        android:layout_alignLeft="@+id/textViewF"
        android:layout_marginTop="10dp"
        android:background="@drawable/back"
        android:padding="5dp"
        android:text="[gender]" />

    <TextView
        android:id="@+id/textViewG"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textViewFT"
        android:layout_alignLeft="@+id/textViewFT"
        android:textStyle="bold"
        android:textAllCaps="true"
        android:text="Alamat" />

    <TextView
        android:id="@+id/textViewGT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewG"
        android:layout_alignLeft="@+id/textViewG"
        android:layout_marginTop="10dp"
        android:padding="5dp"
        android:background="@drawable/back"
        android:text="[address]" />
    <!--    -->




    <Button
        android:id="@+id/button1"
        android:layout_width="@dimen/collapse_toolbar_height"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewGT"
        android:text="Kembali"
        style="?android:attr/borderlessButtonStyle"
        android:layout_alignParentBottom="true"
        />

</RelativeLayout>
